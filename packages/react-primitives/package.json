{
  "name": "@slashid/react-primitives",
  "version": "0.6.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slashid/javascript.git"
  },
  "description": "Primitive components for the /id React SDK",
  "homepage": "https://slashid.dev",
  "keywords": [
    "slashid",
    "/id",
    "react",
    "auth",
    "authentication",
    "webauthn",
    "passwordless",
    "jwt"
  ],
  "type": "module",
  "types": "dist/main.ts",
  "main": "src/main.ts",
  "module": "src/main.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./src/main.ts"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./src/main.ts"
      }
    }
  },
  "sideEffects": [
    "./src/components/theme-root/theme.css.ts"
  ],
  "files": [
    "src/*",
    "dist/*",
    "README.md",
    "vite.config.ts",
    "vitest.config.ts",
    "index.html"
  ],
  "scripts": {
    "dev": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "build": "tsc",
    "build:storybook": "storybook build",
    "lint": "eslint ./src",
    "test:types": "tsc --noEmit --emitDeclarationOnly false",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "publish:yalc": "node ./yalc.next.js && pnpm build && yalc publish"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-select": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.1",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "compress.js": "^1.2.2",
    "country-flag-emoji-polyfill": "^0.1.4",
    "country-list-with-dial-code-and-flag": "^3.0.2",
    "tiny-cookie": "^2.5.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@slashid/slashid": "3.17.1",
    "@storybook/addon-essentials": "7.6.19",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-storysource": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compress.js": "^1.1.1",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vanilla-extract/vite-plugin": "^4.0.9",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^1.2.1",
    "cross-fetch": "^4.0.0",
    "jsdom": "^20.0.2",
    "msw": "^1.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.4",
    "storybook": "7.6.9",
    "tsconfig": "workspace:*",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vitest-canvas-mock": "^0.3.3",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "@slashid/slashid": ">= 3.17.1",
    "react": ">=16",
    "react-dom": ">=16"
  }
}
