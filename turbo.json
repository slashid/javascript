{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "test:types": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test:unit": {
      "dependsOn": ["test:types", "lint"],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": [],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "test:types", "lint"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:storybook": {
      "outputs": ["storybook-static/**"]
    },
    "dev": {
      "cache": false
    },
    "serve": {
      "persistent": true
    }
  }
}
